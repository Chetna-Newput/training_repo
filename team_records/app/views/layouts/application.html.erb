<!DOCTYPE html>
<html>
<head>
  <title>Team</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "team/header" %>
  <div class="row">
    <div class="col-sm-3">
      <%= render "team/sidepanel" %>
    </div>
    <div class="col-sm-9">
      <div class="content">
        <%= yieldd %>
      </div>    
    </div>
  </div>
</body>
</html>
