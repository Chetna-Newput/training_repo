      <td><%= link_to 'Show', cricket_team_path(team)  %></td>
      <td><%= link_to 'Edit', edit_cricket_team_path(team) %></td>
      <td><%= link_to 'Delete', delete_cricket_team_path(team), method: :delete,Â data: { confirm: 'Are you sure?' } %></td>
